---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Chris Mytton - Blog</title>
 <link href="https://www.chrismytton.uk/index.xml" rel="self"/>
 <link href="https://www.chrismytton.uk/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://www.chrismytton.uk/</id>
 <author>
   <name>Chris Mytton</name>
   <email>chrismytton@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://www.chrismytton.uk{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://www.chrismytton.uk{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
